{
  "name": "sproto-helper",
  "displayName": "SprotoHelper",
  "description": "Help sproto development",
  "publisher": "Oliver256",
  "version": "0.0.3",
  "icon": "images/sproto.png",
  "engines": {
    "vscode": "^1.89.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:O256/sproto-helper.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sproto",
        "aliases": [
          "sproto",
          "sproto"
        ],
        "extensions": [
          ".sproto"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sproto",
        "scopeName": "source.sproto",
        "path": "./syntaxes/sproto.tmLanguage"
      }
    ]
  }
}